{
  "name": "css-fruit",
  "description": "A template for creating npm package repo",
  "version": "0.1.0",
  "author": "Rainfore <rainforest92@126.com>",
  "scripts": {
    "lint": "eslint ./src --fix",
    "precommit": "node node_modules/vusion-hooks/precommit",
    "build": "webpack-cli",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha --require ts-node/register test/unit.test.ts",
    "test:one": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' mocha --require ts-node/register test/dataTypes/URL.ts"
  },
  "main": "./dist/css-fruit.js",
  "repository": "vusion/css-fruit",
  "bugs": {
    "url": "http://github.com/vusion/css-fruit/issues"
  },
  "license": "MIT",
  "keywords": [
    "js",
    "ts",
    "repo",
    "template"
  ],
  "tags": [
    "js",
    "ts",
    "repo",
    "template"
  ],
  "dependencies": {
    "postcss-value-parser": "^3.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-vusion": "^2.1.3",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "ts-loader": "^5.3.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1",
    "vusion-hooks": "^0.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  }
}
